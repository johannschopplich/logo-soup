{
  "name": "logo-soup",
  "type": "module",
  "version": "0.2.3",
  "packageManager": "pnpm@10.30.0",
  "description": "Normalize logo dimensions for visual balance",
  "author": "Johann Schopplich <hello@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/logo-soup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/logo-soup.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/logo-soup/issues"
  },
  "keywords": [
    "logo",
    "image",
    "analysis",
    "normalization",
    "visual-balance",
    "sharp",
    "svg",
    "png"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "types": "./dist/index.d.mts",
  "bin": {
    "logo-soup": "bin/logo-soup.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "node src/cli.ts",
    "build": "tsdown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "release": "bumpp"
  },
  "dependencies": {
    "ansis": "^4.2.0",
    "citty": "^0.2.1",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@types/node": "^24.10.13",
    "bumpp": "^10.4.1",
    "eslint": "^10.0.0",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "sharp"
    ]
  }
}
